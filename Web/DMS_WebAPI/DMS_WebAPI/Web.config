  <system.webServer>
    <rewrite>
      <rules>
        <rule name="ApiToken">
          <match url="api/token" />
          <action type="Rewrite" url="/token" />
        </rule>
        <rule name="ApiToken2">
          <match url="api/v2/token" />
          <action type="Rewrite" url="/token" />
        </rule>
      </rules>
    </rewrite>
    <modules>
      <remove name="FormsAuthentication" />
    </modules>
    
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="4294967295" />
      </requestFiltering>
    </security>
  <handlers>
